<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Veranstaltung</fullName>
    <active>true</active>
    <errorConditionFormula>AND(CONTAINS(TEXT(LeadSource), &apos;Veranstaltung&apos;), Vortrag__c = &apos;&apos;)</errorConditionFormula>
    <errorDisplayField>Vortrag__c</errorDisplayField>
    <errorMessage>Bitte die genaue Veranstaltung angeben</errorMessage>
</ValidationRule>
